{
    "name": "two-thirds/laravel-test-suite",
    "description": "",
    "keywords": [],
    "homepage": "https://www.gitlab.com/two-thirds/laravel-test-suite.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Hoff",
            "email": "jhoff@twothirds.io"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.4|6.*",
        "phpmd/phpmd": "^2.8",
        "symfony/console": "^3.2|4.*",
        "symfony/process": "^3.2|4.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "mockery/mockery": "^1.2",
        "php-mock/php-mock-phpunit": "^2.1",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "two-thirds/artisan-anywhere": "^1.2"
    },
    "config": {
        "secure-http": true,
        "preferred-install": "dist",
        "prefer-stable": true,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "TwoThirds\\TestSuite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoThirds\\Testing\\": "tests"
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../*"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TwoThirds\\TestSuite\\TestSuiteServiceProvider"
            ]
        }
    }
}
